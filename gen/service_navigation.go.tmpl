{{- $ := . }}
{{- $x := .X }}
{{- $xSymType := call $.SymFromName $x.Name }}
{{- range $y := $x.Navigations }}
{{- $ySymName := call $.SymFromName $y.Name }}
{{- $ySymType := call $.SymFromType $y.Type }}
// {{$ySymName}} returns service endpoint for {{$ySymType}}
func (s *Service{{$xSymType}}) {{$ySymName}}() *Service{{$ySymType}} {
	ss := &Service{{$ySymType}}{ServiceBase: s.ServiceBase}
	ss.baseURL += "/{{$y.Name}}"
	return ss
}
{{- end }}
