{{- $ := . }}
{{- $x := .X }}
{{- $xSymName := call $.SymFromName $x.Name }}
{{- $xSymType := call $.SymFromType $x.Type }}
// {{$xSymName}} returns service singleton endpoint for {{$xSymType}}
func (s *Service) {{$xSymName}}() *Service{{$xSymType}} {
	ss := &Service{{$xSymType}}{ServiceBase: s.ServiceBase}
	ss.baseURL += "/{{$x.Name}}"
	return ss
}
